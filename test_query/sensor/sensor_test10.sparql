SELECT ?X ?TimeStamp ?Value ?Unit ?Installation_Date ?Last_Maintenance ?Next_Maintenance WHERE {
	?X <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://www.w3.org/ns/sosa#Observation> .
	?X <http://qudt.org/schema/qudt#resultTime> ?TimeStamp .
	?X <http://www.w3.org/ns/sosa#hasResult> ?Y .
	?Y <http://qudt.org/schema/qudt#numericValue> ?Value .
	?Y <http://qudt.org/schema/qudt#unit> ?Unit .
	?Y <http://qudt.org/schema/qudt#numericValue> ?Value .
	?Y <http://qudt.org/schema/qudt#numericValue> ?Value .
    ?Y <http://qudt.org/schema/qudt#resultTime> ?Installation_Date .
    ?Y <http://qudt.org/schema/qudt#resultTime> ?Last_Maintenance .
    ?Y <http://qudt.org/schema/qudt#resultTime> ?Next_Maintenance .
}